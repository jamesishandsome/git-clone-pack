{
	"name": "git-clone-pack",
	"version": "0.0.3",
	"description": "> Download and extract a git repository (GitHub, GitLab, Bitbucket) from node.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jamesishandsome/git-download-pack.git"
	},
	"type": "commonjs",
	"devDependencies": {
		"@biomejs/biome": "1.7.2",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/react": "^18.3.2",
		"execa": "^9.1.0",
		"fs-extra": "^11.2.0",
		"glob": "^10.3.15",
		"husky": "^9.0.11",
		"rollup": "4.24.4",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"typescript": "^5.4.5",
		"vitepress": "^1.2.2"
	},
	"files": ["dist"],
	"keywords": ["cli", "terminal", "term", "console", "git", "download-git-repo"],
	"scripts": {
		"prepare": "husky",
		"test": "echo \"Error: no test specified\" && exit 0",
		"docs:dev": "vitepress dev",
		"docs:build": "vitepress build",
		"docs:preview": "vitepress preview",
		"format": "biome format --write .",
		"lint": "biome lint ./packages",
		"build": "rm -rf dist && rollup -c && yalc push"
	},
	"author": "James Hu",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jamesishandsome/git-download-pack/issues"
	},
	"homepage": "https://github.com/jamesishandsome/git-download-pack#readme",
	"dependencies": {
		"execa": "^9.1.0"
	}
}
